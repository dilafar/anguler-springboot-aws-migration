<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-check-suppression.1.4.xsd"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="https://jeremylong.github.io/DependencyCheck/dependency-check-suppression.1.4.xsd https://jeremylong.github.io/DependencyCheck/dependency-check-suppression.1.4.xsd">

    <!-- Suppression for specific CVEs -->
    <suppress>
        <cve>CVE-2024-45801</cve>
        <notes>This CVE is suppressed temporarily as it is mitigated by runtime configurations.</notes>
    </suppress>

    <suppress>
        <cve>CVE-2024-47875</cve>
        <notes>This CVE is suppressed temporarily while awaiting a patched version.</notes>
    </suppress>

    <!-- Suppression for vulnerabilities in specific dependencies -->
    <suppress>
        <dependency>
            <name>swagger-ui</name>
        </dependency>
        <cve>CVE-2024-45801</cve>
        <cve>CVE-2024-47875</cve>
        <notes>Swagger-UI dependency with known vulnerabilities; safe under current use case.</notes>
    </suppress>

    <!-- Suppression for vulnerabilities in specific files -->
    <suppress>
        <filePath regex="true">.*swagger-ui-bundle\.js</filePath>
        <cve>CVE-2024-45801</cve>
        <cve>CVE-2024-47875</cve>
    </suppress>

    <suppress>
        <filePath regex="true">.*swagger-ui-es-bundle\.js</filePath>
        <cve>CVE-2024-45801</cve>
        <cve>CVE-2024-47875</cve>
    </suppress>

</suppressions>
